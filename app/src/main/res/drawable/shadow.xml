<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#00000000"/>
            <corners android:radius="12dp"/>
            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp"/>
            <stroke android:color="#4D8BC34A" android:width="4dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#FFFFFF"/>
            <corners android:radius="12dp"/>
            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp"/>
            <stroke android:color="#8BC34A" android:width="2dp"/>
        </shape>
    </item>
</layer-list>
